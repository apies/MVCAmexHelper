


<table>
    <tr>
        <th>
            Amount
        </th>
        <th>
            Cardholder
        </th>
        <th>
            DateofPurchase
        </th>
        <th>
            Approved
        </th>
        <th>
            Billable
        </th>
        <th>
            ApprovedToPush
        </th>
        <th></th>
    </tr>



@foreach (var item in ViewBag.Charges) {
    <tr>
        <td>
         @item.Amount
        </td>
        <td>
         @item.Cardholder
        </td>
        <td>
            @item.DateofPurchase
        </td>
        <td>
            @item.Approved
        </td>
        <td>
            @item.Billable
        </td>
        <td>
            @item.ApprovedToPush
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>